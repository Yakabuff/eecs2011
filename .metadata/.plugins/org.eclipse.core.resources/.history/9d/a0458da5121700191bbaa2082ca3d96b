		try {
		
		
		BufferedWriter output = new BufferedWriter(new FileWriter(file));          
		output.write("abc");
		output.close();
		}catch(IOException e ){
			
		}

		try {
		
		FileWriter fileWriter = new FileWriter(file, true);
		BufferedWriter output = new BufferedWriter(fileWriter);          
		output.write("def");
		output.close();
		}catch(IOException e ){
			
		}
		
		
	}

}
